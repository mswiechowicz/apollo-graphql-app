# Auth
JWT_SECRET=shhh_secret!
JWT_EXPIRATION_TIME=6h

// queries
query Query {
  users {
    id
    username
    password
    email
    privilege
    active
    createdAt
    updatedAt
  }
}

mutation Mutation($input: LoginInput) {
  login(input: $input) {
    token
  }
}


mutation CreateUser($createUserInput2: CreateUserInput) {
  createUser(input: $createUserInput2) {
    id
    username
    password
    email
    privilege
    active
    createdAt
    updatedAt
  }
}

query UserData($at: String!) {
  userData(at: $at) {
    id
    username
    email
    privilege
    active
    createdAt
    password
    updatedAt
  }
}

